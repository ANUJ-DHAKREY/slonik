{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {"ecmaVersion": 2018, "sourceType": "module"},
  "extends": [
  ],
  "root": true,
  "rules": {
    "object-curly-newline": [
      2,
      {
        "ExportDeclaration": {
          "minProperties": 1,
          "multiline": true
        },
        "ImportDeclaration": {
          "minProperties": 1,
          "multiline": true
        }
      }
    ],
    "import/no-cycle": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "no-unused-expressions": [
      2,
      {
        "allowTaggedTemplates": true
      }
    ]
  }
}
